  "scripts": {
    "sonar": "sonar-scanner",
    "dev": "vite",
    "build": "tsc --project tsconfig.node.json && vite build",
    "build:run": "serve -s dist",
    "build:deploy": "gsutil -m cp -r dist/* gs://undian-fe",
    "start": "vite preview",
    "start:dev": "npm run build && npm run start",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "clear-all": "rm -rf node_modules .next out dist build",
    "re-start": "rm -rf node_modules .next out dist build && yarn install && yarn dev",
    "re-build": "rm -rf node_modules .next out dist build && yarn install && yarn build",
    "re-build-test": "rm -rf node_modules .next out dist build && npm install && npm run build",
    "docker:dev:build": "docker buildx build --platform=linux/amd64 -t asia-southeast2-docker.pkg.dev/ptpl-playground/docker/undian-fe-dev:latest -f devops/Dockerfile.stag .",
    "docker:dev:push": "docker push asia-southeast2-docker.pkg.dev/ptpl-playground/docker/undian-fe-dev:latest",
    "cloudrun:dev:deploy": "gcloud run deploy promo-undian --image asia-southeast2-docker.pkg.dev/ptpl-playground/docker/undian-fe-dev:latest --platform managed --region asia-southeast2 --project ptpl-playground --allow-unauthenticated",
    "docker:prd:build": "docker buildx build --platform=linux/amd64 -t asia-southeast2-docker.pkg.dev/ptpl-devops-prod/docker-prod/undian-fe-prod:latest -f devops/Dockerfile.prod .",
    "docker:prd:push": "docker push asia-southeast2-docker.pkg.dev/ptpl-devops-prod/docker-prod/undian-fe-prod:latest",
    "cloudrun:prd:deploy": "gcloud run deploy promo-undian --image=asia-southeast2-docker.pkg.dev/ptpl-devops-prod/docker-prod/undian-fe-prod:latest --service-account=postit@ptpl-devops-prod.iam.gserviceaccount.com --region=asia-southeast2 --project=ptpl-devops-prod && gcloud run services update-traffic promo-undian --region=asia-southeast2 --to-latest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
